<h2>There are {{posts.length}} posts </h2>
<div ng-repeat='post in posts'>
  <h4> {{post.title}}</h4>
  <p> {{post.text}}</p>
	<a href="#/readPost/{{post.id}}"><button type="btn" class="btn btn-default">Read</button></a>
	<a href="#/editPost/{{post.id}}"><button type="btn" class="btn btn-default">Edit</button></a>
	<a href="#/deletePost/{{post.id}}"><button type="btn" class="btn btn-default">Delete</button></a>
 </div>